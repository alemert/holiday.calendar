
# ------------------------------------------------------------------------------
# commands
# ------------------------------------------------------------------------------
CC = gcc -c
LL = gcc

# ------------------------------------------------------------------------------
# options
# ------------------------------------------------------------------------------
CCOPT = -Wall
LLOPT = 

# ------------------------------------------------------------------------------
# rules
# ------------------------------------------------------------------------------


all: dateinfo

% : %.o
	$(LL) $(LLOPT) $^ -o $@

%.o : %.c
	$(CC) $(CCOPT) $^ -o  $@


