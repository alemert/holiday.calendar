
# ------------------------------------------------------------------------------
# commands
# ------------------------------------------------------------------------------
CC = gcc -c
LL = gcc

# ------------------------------------------------------------------------------
# options
# ------------------------------------------------------------------------------
CCOPT = -Wall $(DBG)
LLOPT = 

DBG = 

# ------------------------------------------------------------------------------
# rules
# ------------------------------------------------------------------------------


all: isholiday

isholliday : main.o feierlib.o objhnd.o 
	$(LL) $(LLOPT) $^ -o  $@
		
%.o : %.c %.h
	$(CC) $(CCOPT) $< -o  $@

clean :
	rm -f *.o
	rm -f isholliday
